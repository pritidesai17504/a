#include<iostream>
#include<bits/stdc++.h>
using namespace std;
vector<int>tmp;
vector<vector<int>>subseqArr;
void subSeq(vector<int>&arr,int i)
{
    if(i>=arr.size(arr[i]))
    {
    tmp.push_back();
    subSeq.push_back();
    return;
    }
    tmp.pushback();
    subSeq(arr,i+1);
    tmp.pop_back();
    subSeq(arr,i+1);
}
int sum=0;
void subseqSum(vector<int>&arr,int i)
{
   if(i>=arr.size(arr[i]))
    {
    cout<<sum<<"\n";
    }
   sum+=arr[i];
    subSeq(arr,i+1);
    sum-=arr[i];
     subSeq(arr,i+1);
}
